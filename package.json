{
  "name": "code-recipes",
  "version": "1.0.0",
  "description": "code-recipes",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --inspect -r dotenv/config --watch 'src/**/*.js' --exec babel-node src/index.js",
    "build": "rimraf dist &&  webpack --env production --mode production",
    "start": "set NODE_ENV=production && node ./dist/main.js",
    "test": "nodemon --exec 'mocha ./src/test/**/*.js'",
    "test-min": "nodemon --exec 'mocha ./src/test/**/*.js -R min'"
  },
  "keywords": [
    "code-recipes"
  ],
  "author": "Itay Schmidt",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.1.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-tools": "^8.0.0",
    "joi": "^17.4.1",
    "mocha": "^9.0.3",
    "mongoose": "^5.13.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "webpack": "^5.47.1",
    "webpack-node-externals": "^3.0.0"
  }
}
